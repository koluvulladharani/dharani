class Solution {
    public int maximumWealth(int[][] accounts) {
        int maxWealth=0;
        for (int[] customer:accounts){
            int currentWealth=0;
            for (int money:customer){
                currentWealth+=money;
            }
            if (currentWealth>maxWealth){
                maxWealth=currentWealth;
            }
            }
            return maxWealth;
    }   
    }
